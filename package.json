{
  "name": "@sikaeducation/stylelint-config",
  "version": "1.1.3",
  "description": "Linting and formatting for Sika's CSS and SCSS projects",
  "main": "index.js",
  "keywords": [
    "stylelint",
    "stylelint-config"
  ],
  "scripts": {
    "update:sika": "npm i @sikaeducation/eslint-config@latest",
    "lint": "eslint --report-unused-disable-directives --ignore-path .gitignore --fix . && prettier . --write"
  },
  "author": "Kyle Coberly",
  "license": "MIT",
  "peerDependencies": {
    "stylelint": "^15.10.3"
  },
  "dependencies": {
    "@ronilaukkarinen/stylelint-a11y": "^1.2.7",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-hudochenkov": "^9.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-plugin-defensive-css": "^0.9.1",
    "stylelint-plugin-logical-css": "^0.13.2",
    "stylelint-use-nesting": "^4.1.0"
  },
  "devDependencies": {
    "@sikaeducation/eslint-config": "^3.1.0",
    "prettier": "^3.0.3"
  }
}
